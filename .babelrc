{
  "presets": [
    "@babel/preset-env"
  ],
  "plugins": [
    "@babel/plugin-syntax-object-rest-spread",
    "@babel/plugin-proposal-object-rest-spread",
    [
      "prismjs",
      {
        "languages": [
          "yaml",
          "diff",
          "docker",
          "javascript",
          "go"
        ],
        "plugins": [
          "line-numbers"
        ],
        "theme": "tomorrow",
        "css": true
      }
    ]
  ]
}
